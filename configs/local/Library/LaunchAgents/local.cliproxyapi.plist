<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>local.cliproxyapi</string>
    
    <key>ProgramArguments</key>
    <array>
      <string>cliproxyapi</string>
      <string>--config</string>
      <string>$HOME/.config/cliproxyapi/config.yaml</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <false/>
    </dict>

    <key>EnvironmentVariables</key>
    <dict>
      <key>PATH</key>
      <string>/opt/homebrew/bin:/opt/homebrew/sbin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/Users/thanhhaikhong/.nix-profile/bin</string>
      <key>HOME</key>
      <string>/Users/thanhhaikhong</string>
    </dict>
    
  </dict>
</plist>
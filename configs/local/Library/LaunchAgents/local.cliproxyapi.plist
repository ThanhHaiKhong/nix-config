<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>local.cliproxyapi</string>
    
    <key>ProgramArguments</key>
    <array>
      <string>/bin/sh</string>
      <string>-c</string>
      <string>exec /opt/homebrew/bin/cliproxyapi --config $HOME/.config/cliproxyapi/config.yaml &gt;&gt; /tmp/cliproxyapi.log 2&gt;&amp;1</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <false/>
    </dict>
    
    <key>EnvironmentVariables</key>
    <dict>
      <key>PATH</key>
      <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:$HOME/.nix-profile/bin</string>
    </dict>
    
    <key>StandardOutPath</key>
    <string>/tmp/cliproxyapi.log</string>
    
    <key>StandardErrorPath</key>
    <string>/tmp/cliproxyapi.log</string>
  </dict>
</plist>